html,body,div,span,applet,object,iframe,h1,h2,h3,h4,h5,h6,p,blockquote,pre,a,abbr,acronym,address,big,cite,code,del,dfn,em,font,img,ins,kbd,q,s,samp,small,strike,strong,sub,sup,tt,var,b,u,i,center,dl,dt,dd,ol,ul,li,fieldset,form,label,legend,caption,tbody,tfoot,thead,tr,th,td { margin:0;padding:0;border:0;outline:0;font-size:100%;vertical-align:baseline;background:transparent; font-weight:normal;}
body { line-height:1; }
ol,ul { list-style:none; }
blockquote,q { quotes:none; }
table { border-collapse:collapse;border-spacing:0; }
input,button,select,textarea{outline:none;font: normal 1.125em '微软雅黑', Georgia, serif;}
.clearfix:before, .clearfix:after{
	content: ' ';
	display: block;
	overflow: hidden;
	visibility: hidden;
	width: 0;
	height: 0;
}
.clearfix:after {
	clear: both;
}
.fl{float:left;}
.fr{float:right;}
body{
	font: normal 1.125em '微软雅黑', Georgia, serif;
	-webkit-text-size-adjust: none;
	-webkit-tap-highlight-color:rgba(0,0,0,0);
}
a{
	text-decoration:none;
}
.c669{color:#6690c7;}
.c333{color:#333;}
html,body{
	height:100%;
	overflow:visible;
}
input{
	-webkit-appearance:none;
	
}


/****page-login****/
.content{
	overflow:hidden;
}
.floor_info{
	background:#efebdf;
	border-bottom:solid 1px #e8e6e1;
	border-top:solid 1px #e1e0dd;
	margin-bottom:9px;
}
.house_wrappe{
	margin:0 auto;
	position:relative;
	overflow-y:auto;
}
.house_box{
	width:100%;
	display:inline-block;
	background:url(../images/house_top.png) top left no-repeat;
	background-size:contain;
}
.floor_info ol li{
	float:left;
	width:19.4%;
	margin-left:3.9%;
	text-align:center;
	position:relative;
}
.floor_info ol li span{
	 display:inline-block;
}
.floor_info ol li.normal{
	background:#75a9e5;
}
.floor_info ol li.disable{
	background:#e8e7e7;
}
.floor_info ol li.choose{
	background:#81cc55;
}
.floor_info ol li.normal h2, .floor_info ol li.normal span{color:#fff;}
.floor_info ol li.disable h2, .floor_info ol li.disable span{color:#666666;}
.floor_info ol li.choose h2, .floor_info ol li.choose span{color:#fff;}
.fixed_button{
	position:fixed;
	bottom:0;
	background:#478ede;
	z-index:500;
}
.building_num ul li a.actived{
	color:#478ede;
	border-bottom:solid 1px #478ede;
}
.floor_info ol li em{display:none;}
.slide_btn{
		transform:rotate(0deg);
		-ms-transform:rotate(0deg); 	/* IE 9 */
		-moz-transform:rotate(0deg); 	/* Firefox */
		-webkit-transform:rotate(0deg); /* Safari 和 Chrome */
		-o-transform:rotate(0deg); 
		-ms-transition:-ms-transform 0.3s linear;
		-moz-transition:-moz-transform 0.13s linear;
		-webkit-transition:-webkit-transform 0.3s linear;
		-o-transition:-o-transform 0.3s linear;
		transition:transform 0.3s linear;
}
.slide_btn.click{
	transform:rotate(180deg);
	-ms-transform:rotate(180deg); 	/* IE 9 */
	-moz-transform:rotate(180deg); 	/* Firefox */
	-webkit-transform:rotate(180deg); /* Safari 和 Chrome */
	-o-transform:rotate(180deg); 
}
.popDiv{
	position:absolute;
	background:#fff;
	width:100%;
	opacity:0.8;
}
.popDiv ul{
	margin-top:5%;
	padding:0 5px;
}
.popDiv ul li{
	float:left;
	line-height:24px;
}
.message_goback{
	position:fixed;
	top:0;
	width:100%;
	background:#85b4e8;
	z-index:10;
}
.message_goback a{
	width:100%;
	height:100%;
	display:block;
	color:#fff;
}
.bottom_submit{
	position:fixed;
	bottom:0;
	width:100%;
	background:#f3f3f3;
}
.message_wrappe{
	width:100%;
	position:relative;
	overflow-y:scroll;
}
.messages_box{
	width:84%;
	margin:0 auto;
}
.leave_message{
	border:solid 1px #e1dede;
	width:90%;
	color:#7c7b7b;
}
.submit_message{
	width:100%;
	background:#478ede;
	border:0;
	color:#fff;
}
.all_messages{
	width:100%;
	border-top:solid 1px #e1dede;
}
.message_list{
	width:83%;
	margin:0 auto;
}
.each_message{
	width:100%;
}
.message_icon{
	display:inline-block;
	background:url(../images/message_icon.png) top no-repeat;
}
.message_name, .fl_message .message_text{
	color:#7c7b7b;
}
.message_time{
	display:block;
	color:#999999;
}
.fl_message .message_text{
	background:#fff;
	border:solid 1px #e1dede;
}
.fr_message .message_text{
	background:#aacff8;
	color:#202020;
	border:solid 1px #e1dede;
}
.fr_message .message_time{text-align:right;}
.fl_message .message_info{
	width:25%;
	text-align:center;
}
.fl_message .message_content{
	width:70%;
	overflow:hidden;
}
.fr_message .message_info{
	width:15%;
	text-align:center;
}
.fr_message .message_content{
	width:80%;
	overflow:hidden;
}
.bottom_nav{
	position:fixed;
	bottom:0;
	border-top:solid 1px #d2d2d2;
	background:#fff;
	width:100%;
}
.bottom_nav ul{height:100%;}
.bottom_nav ul li{
	float:left;
	float:left;
	width:25%;
	height:100%;
}
.bottom_nav ul li a{
	display:block;
	height:100%;
	text-align:center;
}
.bottom_nav ul li i{
	display:inline-block;
}
.bottom_nav ul li.nav_actived p{
	color:#478ede;
}
.button_value{display:block;height:100%;text-align:center;position:relative;}
.button_value input[type="submit"]{
	background:0 none;
	width:100%;
	height:100%;
	border:0;
	text-align:center;
	color:#fff;
}
.button_value i{
	position:absolute;
	top:0;
	right:35%;
	color:#fff;
}
.button_value a{display:block;
width:100%; color:#fff;}
.all_link{display:block;}
.all_link_span{color:#6690c7;}
